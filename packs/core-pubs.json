            "condition": "$P_Suitable === 'Yes' && $P_FormalModeling === 'NotApplicable'"
        },
        {
            "id": "P_PreregisteredReplication",
            "type": "radio",
            "title": "The paper contains a preregistered replication attempt (either direct/close or conceptual)",
            "options": [
                {
                    "id": "NotApplicable",
                    "text": "Not applicable [provide explanation]"
                },
                {
                    "id": "No",
                    "text": "No"
                },
                {
                    "id": "Yes",
                    "text": "Yes"
                }
            ],
            "info": "As only <i>preregistered</i> replication attempts count, you need to provide the DOI or URL of the preregistration. If the paper contains a replication which was not preregistered, please select 'No'.",
            "condition": "$P_Suitable === 'Yes'"
        },
        {
            "id": "P_PreregisteredReplication_NAExplanation",
            "type": "text",
            "title": "⤷ Not applicable: Explanation",
            "condition": "$P_Suitable === 'Yes' && $P_PreregisteredReplication === 'NotApplicable'"
        },
        {
            "id": "P_PreregisteredReplication_Identifier",
            "type": "text",
            "title": "⤷ DOI or URL of the preregistration",
            "condition": "$P_Suitable === 'Yes' && $P_PreregisteredReplication === 'Yes'"
