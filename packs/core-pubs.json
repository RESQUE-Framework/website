        {
            "id": "P21P22SEP",
            "type": "separator",
            "condition": "$P_Suitable === 'Yes'"
        },
        {
            "id": "P_PowerConsiderations",
            "type": "textbox",
            "title": "Please report all considerations regarding statistical power (assumed effect size, power, N, ...)",
            "condition": "$P_Suitable === 'Yes'"
