            "condition": "$P_TypeMethod_OtherMethod"
        },
        {
            "id": "P_TypePublication",
            "type": "radio",
            "title": "Publication type",
            "options": [
                {
                    "id": "PublishedPaper",
                    "text": "Published paper / Paper in press"
