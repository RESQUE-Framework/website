            "id": "P5P6SEP",
            "type": "separator"
        },
        {
            "id": "P_TypeMethod",
            "type": "checkbox",
            "title": "Publication type (check all that apply)",
            "options": [
                {
                    "id": "Empirical",
                    "text": "Empirical paper"
                },
                {
                    "id": "MetaAnalysis",
                    "text": "Meta-Analysis"
                },
                {
                    "id": "Theoretical",
                    "text": "(Mainly) Theoretical contribution / Review"
                },
