                {
                    "id": "No",
                    "text": "No"
                },
                {
                    "id": "Yes",
                    "text": "Yes"
                }
            ],
            "condition": "$P_Suitable === 'Yes'"
        },
        {
            "id": "P_FormalModeling_NAExplanation",
            "type": "text",
