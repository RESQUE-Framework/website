                },
                {
                    "id": "NoRole",
                    "text": "No Role"
                },
                {
                    "id": "NA",
                    "text": "n/a"
                }
            ],
            "default": "NoRole",
            "alignment": "horizontal",
            "tooltip": "Ideas; formulation or evolution of overarching research goals and aims.",
            "info": "Ideas; formulation or evolution of overarching research goals and aims."
        },
