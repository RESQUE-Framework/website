        {
            "id": "P_Data_Open",
            "type": "radio",
            "title": "⤷ Is the raw data available in a repository that also controls data access (not upon request)",
            "options": [
                {
                    "id": "NotApplicable",
                    "text": "Not applicable [provide explanation]"
                },
                {
                    "id": "NotAvailable",
                    "text": "Not available"
                },
                {
                    "id": "YesParts",
                    "text": "Yes, parts of data [provide doi or URL]"
                },
                {
                    "id": "YesEntire",
                    "text": "Yes, entire data [provide doi or URL]"
                }
            ],
            "condition": "$P_Suitable === 'Yes' && $P_Data === 'Yes' && $P_Data_Source_NewOwn"
        },
        {
            "id": "P_Data_Open_NAExplanation",
            "type": "text",
            "title": "⤷ Not Applicable: Explanation",
            "condition": "$P_Suitable === 'Yes' && $P_Data === 'Yes' && $P_Data_Source_NewOwn && $P_Data_Open === 'NotApplicable'"
        },
        {
            "id": "P_Data_Open_Identifier",
            "type": "text",
            "title": "⤷ DOI or URL",
            "condition": "$P_Suitable === 'Yes' && $P_Data === 'Yes' && $P_Data_Source_NewOwn && $P_Data_Open =|= ['YesParts', 'YesEntire']"
        },
        {
            "id": "P_Data_Open_AccessLevel",
            "type": "radio",
            "title": "⤷ Access level",
            "options": [
                {
                    "id": "ZK0",
                    "text": "ZK0 (open data)"
                },
                {
                    "id": "ZK1",
                    "text": "ZK1 (scientific use file)"
                },
                {
                    "id": "ZK2",
                    "text": "ZK2 (restricted access) [provide explanation]"
                }
            ],
            "condition": "$P_Suitable === 'Yes' && $P_Data === 'Yes' && $P_Data_Source_NewOwn && $P_Data_Open === 'YesEntire'"
        },
        {
            "id": "P_Data_Open_AccessLevel_ZK2Explanation",
            "type": "text",
            "title": "⤷ Access level ZK2: Explanation",
            "condition": "$P_Suitable === 'Yes' && $P_Data === 'Yes' && $P_Data_Source_NewOwn && $P_Data_Open === 'YesEntire' && $P_Data_Open_AccessLevel === 'ZK2'"
        },
        {
            "id": "P_Data_Open_FAIR",
            "type": "checkbox",
            "title": "⤷ FAIR format",
            "options": [
                {
                    "id": "PersistentIdentifier",
                    "text": "The data set has a globally unique, citable and persistent identifier (e.g., DOI, PURL, ARK)"
                },
                {
                    "id": "Codebook",
                    "text": "All variables are comprehensively described in a codebook with a structured format"
                },
                {
                    "id": "StructuredOpenFormat",
                    "text": "The data set is stored in a structured, open format (e.g., .csv, .tsv, .tab, .txt, .json; not .sav, .xlsx, .pdf)"
                }
            ],
            "condition": "$P_Suitable === 'Yes' && $P_Data === 'Yes' && $P_Data_Source_NewOwn && $P_Data_Open === 'YesEntire' && ($P_Data_Open_AccessLevel =|= ['ZK0', 'ZK1'] || $P_Data_Open_AccessLevel === 'ZK2' && $P_Data_Open_AccessLevel_ZK2Explanation)"
        },
        {
            "id": "P13P14SEP",
            "type": "separator",
            "condition": "$P_Suitable === 'Yes'"
        },
        {
            "id": "P_IndependentVerification",
            "type": "radio",
            "title": "Correctness of computational results has been independently verified",
            "options": [
                {
                    "id": "NotApplicable",
                    "text": "Not applicable [provide explanation]"
                },
                {
                    "id": "No",
                    "text": "No"
                },
                {
                    "id": "WorkflowReproducible",
                    "text": "Workflow reproducibility / code completeness check"
                },
                {
                    "id": "MainResultsReproducible",
                    "text": "Reproducibility of main results has been independently verified using the original code"
                },
                {
                    "id": "AllResultsReproducible",
                    "text": "Reproducibility of all numerical results has been independently verified using the original code"
                },
                {
                    "id": "AnalysisReplication",
                    "text": "Analysis replication (independent implementation & verification)"
                }
            ],
            "eval_dimension": "rigor",
            "info": "<ul><li>{:'WorkflowReproducible' | <b>Workflow reproducibility / code completeness check</b>: Code runs without errors or critical warnings on another machine by another person (without comparing the numbers). This is also known as <i>process reproducibility</i>.}</li><li>{:'MainResultsReproducible' | <b>Reproducibility of main results has been independently verified using the original code</b>: Someone compared the <i>figures and main results</i> and asserts a sufficient correspondence with the reported main results.}</li><li>{:'AllResultsReproducible' | <b>Reproducibility of <i>all numerical results</i> has been independently verified using the original code</b>: Someone compared <i>all figures and numbers</i> and asserts a sufficient correspondence with the reported results.}</li><li>{:'AnalysisReplication' | <b>Analysis replication (independent implementation)</b>: Somebody independently implemented the analyses, without looking at the original code, and asserts a sufficient correspondence with the reported main results.}</li></ul><br><br>{:'WorkflowReproducible', 'MainResultsReproducible', 'AllResultsReproducible', 'AnalysisReplication' | The bonus point is only given when a public verification report/certificate with a persistent identifier is available.}",
            "background": "'In the context of mathematics, Stodden et al. distinguish between <i>verification</i> and <i>validation</i>; following their definition, <b>a CODECHECK [or reproducibility check] ensures verification of computational results, i.e., checking that code generates the output it claims to create</b>, but <i>not</i> a validation, i.e., checking that the code implements the right algorithm [or statistical model] to solve the specific research problem.' (see <a href='https://f1000research.com/articles/10-253/v2'>Nüst & Eglen, 2021</a>).",
            "condition": "$P_Suitable === 'Yes'"
        },
        {
            "id": "P_IndependentVerification_NAExplanation",
            "type": "text",
            "title": "⤷ Not Applicable: Explanation",
            "condition": "$P_Suitable === 'Yes' && $P_IndependentVerification === 'NotApplicable'"
        },
        {
            "id": "P_IndependentVerification_Identifier",
            "type": "text",
            "title": "⤷ DOI or URL of the verification report",
            "condition": "$P_Suitable === 'Yes' && $P_IndependentVerification =|= ['WorkflowReproducible', 'MainResultsReproducible', 'AllResultsReproducible', 'AnalysisReplication']"
        },
        {
            "id": "P_ReproducibleScripts",
            "type": "radio",
            "title": "Open reproducible scripts",
            "options": [
                {
                    "id": "NotApplicable",
                    "text": "Not applicable [provide explanation]"
                },
                {
                    "id": "NotAvailable",
                    "text": "Not available"
                },
                {
                    "id": "YesParts",
                    "text": "Yes, parts of scripts [provide doi or URL]"
                },
                {
                    "id": "YesEntire",
                    "text": "Yes, entire scripts [provide doi or URL]"
                }
            ],
            "condition": "$P_Suitable === 'Yes'"
        },
        {
            "id": "P_ReproducibleScripts_NAExplanation",
            "type": "text",
            "title": "⤷ Not applicable: Explanation",
            "condition": "$P_Suitable === 'Yes' && $P_ReproducibleScripts === 'NotApplicable'"
        },
        {
            "id": "P_ReproducibleScripts_Identifier",
            "type": "text",
            "title": "⤷ DOI or URL of the script",
            "condition": "$P_Suitable === 'Yes' && $P_ReproducibleScripts =|= ['YesParts', 'YesEntire']"
        },
        {
            "id": "P_ReproducibleScripts_FAIR",
            "type": "checkbox",
            "title": "⤷ FAIR format",
            "options": [
                {
                    "id": "TimestampedRepository",
                    "text": "Timestamped repository"
                },
                {
                    "id": "VersionControl",
                    "text": "Version control"
                },
                {
                    "id": "ReproducibleManuscripts",
                    "text": "Reproducible manuscripts (e.g. with R Markdown)"
                },
                {
                    "id": "Documentation",
                    "text": "Proper documentation of code"
                },
                {
                    "id": "ReproducibleSoftwareEnvironments",
                    "text": "Reproducible software environments (e.g. conda environment, renv environment in R)"
                }
            ],
            "condition": "$P_Suitable === 'Yes' && $P_ReproducibleScripts =|= ['YesParts', 'YesEntire']"
        },
        {
            "id": "P_OpenMaterials",
            "type": "radio",
            "title": "Open materials (beyond the open reproducible scripts entered above)",
            "options": [
                {
                    "id": "NotApplicable",
                    "text": "Not applicable [provide explanation]"
                },
                {
                    "id": "NotAvailable",
                    "text": "Not available"
                },
                {
                    "id": "YesParts",
                    "text": "Yes, parts of material [provide doi or URL]"
                },
                {
                    "id": "YesEntire",
                    "text": "Yes, entire material [provide doi or URL]"
                }
            ],
            "info": "{ :'YesParts', 'YesEntire' | Only choose 'Yes' if the material substantially exceeds the open reproducible scripts. }<br><br>{:0 | When to choose 'Not applicable': If no material has been used in the publication, or the entire material is contained in the open reproducible scripts. }",
            "condition": "$P_Suitable === 'Yes'"
        },
        {
            "id": "P_OpenMaterials_NAExplanation",
            "type": "text",
            "title": "⤷ Not applicable: Explanation",
            "condition": "$P_Suitable === 'Yes' && $P_OpenMaterials === 'NotApplicable'"
        },
        {
            "id": "P_OpenMaterials_Identifier",
            "type": "text",
            "title": "⤷ DOI or URL of the open materials",
            "condition": "$P_Suitable === 'Yes' && $P_OpenMaterials =|= ['YesParts', 'YesEntire']"
        },
        {
            "id": "P17P18SEP",
            "type": "separator",
            "condition": "$P_Suitable === 'Yes'"
        },
        {
            "id": "P_Preregistration",
